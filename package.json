{
  "name": "ai-cosearch-chrome-extension",
  "version": "1.0.0",
  "description": "Google Chrome extension that provides an assistant to complement search",
  "main": "index.js",
  "scripts": {
    "prebuild": "npm run clean:dist && npm run build:manifest",
    "build": "NODE_ENV=production webpack",
    "build:manifest": "ts-node-dev --clear ./scripts/build-manifest.ts",
    "prestart": "npm run prebuild",
    "start": "NODE_ENV=development webpack watch",
    "lint:code": "eslint . --ext ts,tsx --cache",
    "lint:typescript": "tsc --noEmit",
    "format": "prettier . --check --cache --ignore-unknown",
    "todo": "npx leasot '**/**' '.**/**' -i '**/node_modules/**' -S -x",
    "clean:dist": "rm -rf dist",
    "clean": "npm run clean:dist && rm -rf node_modules"
  },
  "keywords": [
    "ai",
    "chrome",
    "extension",
    "assistant",
    "copilot"
  ],
  "author": "Abel Lopes (@abelflopes)",
  "license": "ISC",
  "dependencies": {
    "react": "^18.2.0",
    "react-ck": "^1.2.5"
  },
  "devDependencies": {
    "@abelflopes/eslint-config-tsr-pro": "^2.0.1",
    "@abelflopes/prettier-config-pro": "^1.0.0",
    "chrome-types": "^0.1.273",
    "copy-webpack-plugin": "^12.0.2",
    "eslint": "^8.57.0",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "glob": "^10.3.10",
    "html-webpack-plugin": "^5.6.0",
    "prettier": "^3.2.5",
    "swc-loader": "^0.2.6",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.2",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "zod": "^3.22.4"
  }
}
